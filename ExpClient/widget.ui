   <string notr="true">/* 主窗口样式 */
QMainWindow {
    background: qlineargradient(x1: 0, y1: 0, x2: 0, y2: 1,
                                stop: 0 #2c3e50, stop: 1 #34495e);
    border: none;
}

/* 顶部时间标签 */
QLabel#TimeLabel {
    background: transparent;
    color: #ecf0f1;
    font-size: 12px;
    font-weight: bold;
    padding: 4px 8px;
    border-radius: 3px;
}

/* 工具栏按钮 */
QToolButton {
    background: transparent;
    border: 1px solid #7f8c8d;
    border-radius: 3px;
    color: #bdc3c7;
    padding: 4px;
    margin: 1px;
}

QToolButton:hover {
    background: rgba(52, 152, 219, 0.3);
    border: 1px solid #3498db;
    color: #ecf0f1;
}

QToolButton:pressed {
    background: rgba(41, 128, 185, 0.5);
}

/* 消息列表视图 */
QListView {
    background: #ecf0f1;
    border: 1px solid #bdc3c7;
    border-radius: 4px;
    font-size: 14px;
    color: #2c3e50;
    padding: 8px;
    outline: none;
    alternate-background-color: #dfe6e9;
}

QListView::item {
    border-bottom: 1px solid #bdc3c7;
    padding: 8px;
}

QListView::item:selected {
    background: #3498db;
    color: white;
    border-radius: 3px;
}

/* 聊天输入框 */
QTextEdit {
    background: #ffffff;
    border: 2px solid #bdc3c7;
    border-radius: 5px;
    font-size: 14px;
    color: #2c3e50;
    padding: 10px;
    font-family: 'Segoe UI', Arial, sans-serif;
}

QTextEdit:focus {
    border: 2px solid #3498db;
    background: #f8f9fa;
}

/* 功能按钮 */
QPushButton {
    background: qlineargradient(x1: 0, y1: 0, x2: 0, y2: 1,
                                stop: 0 #3498db, stop: 1 #2980b9);
    border: none;
    border-radius: 4px;
    color: white;
    font-size: 12px;
    font-weight: bold;
    padding: 8px 12px;
    margin: 2px;
    min-width: 80px;
}

QPushButton:hover {
    background: qlineargradient(x1: 0, y1: 0, x2: 0, y2: 1,
                                stop: 0 #2980b9, stop: 1 #2573a7);
}

QPushButton:pressed {
    background: qlineargradient(x1: 0, y1: 0, x2: 0, y2: 1,
                                stop: 0 #2573a7, stop: 1 #1f618d);
}

/* 发送按钮特殊样式 */
QPushButton#pushButton_emission {
    background: qlineargradient(x1: 0, y1: 0, x2: 0, y2: 1,
                                stop: 0 #27ae60, stop: 1 #229954);
}

QPushButton#pushButton_emission:hover {
    background: qlineargradient(x1: 0, y1: 0, x2: 0, y2: 1,
                                stop: 0 #229954, stop: 1 #1e7e34);
}

/* 停止按钮特殊样式 */
QPushButton#pushButton_2 {
    background: qlineargradient(x1: 0, y1: 0, x2: 0, y2: 1,
                                stop: 0 #e74c3c, stop: 1 #c0392b);
}

QPushButton#pushButton_2:hover {
    background: qlineargradient(x1: 0, y1: 0, x2: 0, y2: 1,
                                stop: 0 #c0392b, stop: 1 #a93226);
}

/* 菜单栏 */
QMenuBar {
    background: #2c3e50;
    color: #ecf0f1;
    border-bottom: 1px solid #34495e;
}

QMenuBar::item {
    background: transparent;
    padding: 5px 10px;
}

QMenuBar::item:selected {
    background: #3498db;
    border-radius: 3px;
}

/* 状态栏 */
QStatusBar {
    background: #2c3e50;
    color: #bdc3c7;
    font-size: 11px;
    padding: 3px;
    border-top: 1px solid #34495e;
}

/* 分隔符和间隔器 */
QSpacerItem {
    background: transparent;
}</string>
