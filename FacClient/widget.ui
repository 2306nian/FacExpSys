   <string notr="true">/* 全局样式 */
QWidget {
    font-family: &quot;Microsoft YaHei&quot;, &quot;SimHei&quot;, sans-serif;
    background-color: #f5f5f5;
    color: #333333;
}

/* 主窗口样式 */
#Widget {
    background-color: qlineargradient(spread:pad, x1:0, y1:0, x2:0, y2:1, 
                      stop:0 #ffffff, stop:1 #f0f0f0);
    border: 1px solid #cccccc;
    border-radius: 8px;
}

/* 标题区域样式 */
#widget_title {
    background-color: transparent;
    min-height: 40px;
}

/* 工具按钮样式 */
#toolButton {
    background-color: transparent;
    border: none;
    border-radius: 4px;
    padding: 4px;
}

#toolButton:hover {
    background-color: rgba(0, 0, 0, 0.1);
}

/* 图标区域样式 */
#widget_icon {
    background-color: transparent;
}

#label_icon {
    border-radius: 45px;
    border: 2px solid #4a86e8;
    padding: 5px;
}

/* 中央区域样式 */
#widget_center {
    background-color: transparent;
}

/* 标签样式 */
QLabel {
    color: #555555;
    font-size: 14px;
    font-weight: bold;
}

/* 输入框样式 */
QLineEdit {
    border: 2px solid #dddddd;
    border-radius: 6px;
    padding: 5px 10px;
    background-color: #ffffff;
    selection-background-color: #4a86e8;
    font-size: 14px;
}

QLineEdit:hover {
    border-color: #bbbbbb;
}

QLineEdit:focus {
    border-color: #4a86e8;
}

/* 登录按钮样式 */
#pushButton_login {
    background-color: qlineargradient(spread:pad, x1:0, y1:0, x2:0, y2:1, 
                      stop:0 #4a86e8, stop:1 #3a76d8);
    color: white;
    border-radius: 6px;
    border: none;
    font-weight: bold;
    font-size: 18px;
}

#pushButton_login:hover {
    background-color: qlineargradient(spread:pad, x1:0, y1:0, x2:0, y2:1, 
                      stop:0 #5a96f8, stop:1 #4a86e8);
}

#pushButton_login:pressed {
    background-color: qlineargradient(spread:pad, x1:0, y1:0, x2:0, y2:1, 
                      stop:0 #3a76d8, stop:1 #2a66c8);
}

#pushButton_login:checked {
    background-color: #2a66c8;
}

/* 单选按钮样式 - 修复版本 */
QRadioButton {
    color: #666666;
    spacing: 5px;
}

QRadioButton::indicator {
    width: 16px;
    height: 16px;
    border: 2px solid #bbbbbb;
    border-radius: 8px;
    background-color: #ffffff;
}

QRadioButton::indicator:hover {
    border-color: #4a86e8;
}

QRadioButton::indicator:checked {
    border: 2px solid #4a86e8;
    background-color: #ffffff;
}

QRadioButton::indicator:checked {
    background-color: qradialgradient(spread:pad, cx:0.5, cy:0.5, radius:0.5, fx:0.5, fy:0.5, 
                           stop:0 #4a86e8, stop:0.5 #4a86e8, stop:0.6 #ffffff);
}

/* 注册按钮样式 */
#pushButton_more {
    background-color: transparent;
    color: #4a86e8;
    border: 1px solid #4a86e8;
    border-radius: 4px;
    padding: 5px 15px;
    font-size: 14px;
}

#pushButton_more:hover {
    background-color: rgba(74, 134, 232, 0.1);
}

#pushButton_more:pressed {
    background-color: rgba(74, 134, 232, 0.2);
}

/* 占位符文本颜色 */
QLineEdit[placeholderText] {
    color: #999999;
}

/* 滚动条样式 */
QScrollBar:vertical {
    border: none;
    background: #f0f0f0;
    width: 8px;
    margin: 0px 0px 0px 0px;
}

QScrollBar::handle:vertical {
    background: #cccccc;
    min-height: 20px;
    border-radius: 4px;
}

QScrollBar::handle:vertical:hover {
    background: #bbbbbb;
}
</string>
